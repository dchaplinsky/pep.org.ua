{% extends "base.jinja" %}
{% from "macros/_connections.jinja" import render_person_banks, render_related_grouped_persons, render_person_companies with context %}
{% from "macros/_articles.jinja" import render_article_snippet with context %}

{% set last_workplace = person.translated_last_workplace %}

{% block og_tags %}
    {% if person.photo %}
        <meta property="og:image" content="{{ SITE_URL }}{{ person.photo.url }}" />
    {% else %}
        <meta property="og:image" content="{{ SITE_URL }}{{ static("images/pep_fb.png") }}" />
    {% endif %}
    <meta property="og:image:alt" content="Politically exposed person {{ person.first_name }} {{ person.patronymic }} {{ person.last_name }}" />
    <meta property="og:type" content="profile" />
    <meta property="og:description" content="{{ _("Детальна інформація про членів сім'ї та інших пов'язаних осіб, задекларовані статки, зобов'язання та банківські рахунки, власність в юридичних особах") }}" />
    <meta property="profile:first_name" content="{{ person.first_name }} {{ person.patronymic }}" />
    <meta property="profile:last_name" content="{{ person.last_name }}" />
{% endblock %}

{% block hreflang %}
    {% if person.get_absolute_url() != request.get_full_path() %}
        <link rel="canonical" href="{{ SITE_URL }}{{ person.get_absolute_url() }}" />
    {% else %}
        {{ super() }}
    {% endif %}
{% endblock %}


{% macro render_lands(lands) %}{% for land in lands %}{{ land }} {{ _("м²") }}<br/>{% endfor %}{% endmacro %}

{% block page_title %}
{{_("Профіль:")}} {{ person.last_name }} {{ person.first_name }} {{ person.patronymic }}{% if last_workplace %}, {{ last_workplace.company }}, {{ last_workplace.position }}{% endif %}
{% endblock %}

{% block og_page_title %}
{{_("Профіль:")}} {{ person.last_name }} {{ person.first_name }} {{ person.patronymic }}{% if last_workplace %}, {{ last_workplace.company }}, {{ last_workplace.position }}{% endif %}
{% endblock %}

{% block extra_menu %}
    {% if request.user.is_superuser %}
        <li class="menu__list-item edit-page">
            <a href="{{ url('admin:core_person_change', person.id) }}" target="_blank">✎</a>
        </li>
    {% endif %}
{% endblock %}

{% block page_description %}{% if person.description %}{{ person.description }}{% endif %}{% endblock %}

{% block page_class %}profile-page{% endblock %}

{% block content %}
{% set related_persons = person.all_related_persons %}
{% set related_countries = person.all_related_countries %}
{% set related_companies = person.all_related_companies %}
{% set workplaces = person.workplaces %}
{% set manhunt_records = person.manhunt_records %}
{% set external_links = person.external_links %}
{% set documents = person.all_documents %}
{% set any_documents = documents.values()|map("length")|sum %}
    <main>
        <div class="profile-page__content" itemscope itemtype="http://schema.org/Person">
            <div class="side-menu-btn">
                <a href="" class="track-ga-event track-ga-event-click" data-ga-event-category="expand" data-ga-event-action="person_toc"><span class="icon"></span>{{ _("зміст") }}</a>
            </div>
            <div class="profile-page__header">
                <div class="breadcrumbs-wrap">
                    <ul class="breadcrumbs">
                        <li>
                            <a href="{{ url('wagtail_serve', '') }}">{{ _("Головна")}} </a>
                        </li>
                        <li>
                            <a href="{{ url('advanced_search') }}">{{ _("Публічні діячі") }}</a>
                        </li>
                        <li>
                            <span class="name"> {{ person.last_name }} {{ person.first_name }} {{ person.patronymic }} </span>
                        </li>
                    </ul>

                    {% set last_modified = person.last_modified %}
                    {% if last_modified %}
                    <div class="date">{{ _("Остання зміна профілю:") }}&nbsp;
                        <time>{{ last_modified | date }}</time>
                    </div>
                    {% endif %}
                </div>
                <div class="profile-actions">
                    <a href="?format=pdf" class="pdf track-ga-event track-ga-event-click" data-ga-event-category="expand" data-ga-event-action="person_pdf" title="pdf">
                        {{ _("PDF") }}
                    </a>
                    <a href="" class="print track-ga-event track-ga-event-click" data-ga-event-category="expand" data-ga-event-action="person_print" title="print">
                        <svg>
                            <use xlink:href="#print"></use>
                        </svg>
                    </a>
                </div>
            </div>

            <div class="profile-page__body">
                <link itemprop="url" href="{{ person.get_absolute_url() }}" />

                <div class="profile-page__top">
                    <div class="profile-image">
                        {% if person.photo %}
                        <img src="{{ person.photo | thumbnail_url("avatar") }}" itemprop="image" alt="{{ person.last_name }} {{ person.first_name }} {{ person.patronymic }}">
                        {% else %}
                        <img src="{{ static('images/profile-empty-individual.svg') }}" alt="photo" class="profile-card__img">
                        {% endif %}
                        {% if person.reputation_sanctions_en or person.reputation_crimes_en or person.reputation_manhunt_en or person.reputation_convictions_en or person.reputation_sanctions_uk or person.reputation_crimes_uk or person.reputation_manhunt_uk or person.reputation_convictions_uk or manhunt_records %}
                        {% elif person.died %}
                        <div class="icon icon--died">
                            <svg>
                                <use xlink:href="#block"></use>
                            </svg>
                        </div>
                        {% endif %}
                    </div>
                    <div class="profile-main-info">
                        <h1 class="profile-name {% if person.terminated %}profile-name--no-active{% endif %}" itemprop="name">{{ person.last_name }} {{ person.first_name }} {{ person.patronymic }}</h1>
                        <table>
                            <tbody>
                            {% if person.also_known_as %}
                            <tr>
                                <td>{{ _("Інші імена") }}</td>
                                <td itemprop="name">{{ person.also_known_as|replace("\n", "<br/>")|safe }}</td>
                            </tr>
                            {% endif %}
                            {% if person.type_of_official %}
                            <tr>
                                <td>{{ _("Категорія") }}</td>
                                <td>{{ _(person.get_type_of_official_display()) }}</td>
                            </tr>
                            {% endif %}
                            {% if person.dob %}
                            <tr>
                                <td>{{ _("Дата народження") }}</td>
                                <td>
                                    <meta itemprop="birthDate" content="{{ person.dob }} "/>
                                    <time> {{ person.date_of_birth }} </time>
                                </td>
                            </tr>
                            {% endif %}
                            {% if person.city_of_birth %}
                            <tr>
                                <td>{{ _("Місце народження") }}</td>
                                <td itemprop="birthPlace">
                                    {{ person.city_of_birth }}
                                </td>
                            </tr>
                            {% endif %}
                            {% for country_section, connections_with_country in related_countries.items() %}
                                <tr>
                                    <td>{{ country_connections_titles[country_section] }}</td>
                                    <td>
                                        {% set comma = joiner(", ") %}
                                        {% for connection in connections_with_country -%}
                                        {{ comma() }}<span class="flag flag-{{ connection.to_country.iso3|lower }}" title="{{ connection.to_country.name }}"></span> {{ connection.to_country.name }}

                                        {% if connection.has_additional_info %}
                                            (
                                            {% set proofs = connection.proofs.all() %}
                                            <div class="additional-info">
                                                {% if date_confirmed_human %}
                                                <span class="confirmed-date">{{ _("Підтверджено: ") }}{{ connection.date_confirmed_human }}</span>
                                                {% endif %}
                                                {% if connection.proofs %}
                                                    {% set comma = joiner(", ") %}
                                                    {% for proof in proofs -%}
                                                        <a href="{% if proof.proof %}{{ proof.proof }}{% else %}{{ proof.proof_document.doc_url }}{% endif %}" class="link confirmed-link">{{ proof.proof_title|default(_("Посилання"), true) }}</a>
                                                    {% endfor %}
                                                {% endif %}
                                            </div>
                                            )
                                        {% endif %}
                                        {%- endfor %}
                                    </td>
                                </tr>
                            {% endfor %}
                            {% if last_workplace %}
                            <tr>
                                <td>{{ _("Остання посада") }}</td>
                                <td>
                                    {% if last_workplace.company_id %}
                                        <a href="{{ url("company_details", company_id=last_workplace.company_id) }}" itemprop="worksFor" itemscope itemtype="http://schema.org/Organization"><span>{{ last_workplace.company }}</span></a>,
                                    {% else %}
                                        <span itemprop="worksFor" itemscope itemtype="http://schema.org/Organization"><span>{{ last_workplace.company }}</span></span>,
                                    {% endif %}

                                    <span itemprop="jobTitle">{{ last_workplace.position }}</span>
                                </td>
                            </tr>
                            {% endif %}
                            {% if person.terminated %}
                                <tr>
                                    {% if person.termination_date %}
                                    <td>{{ person.termination_date_human }}</td>
                                    {% else %}
                                    <td></td>
                                    {% endif %}
                                    <td class="terminated">{% if person.died %}<strong>{% endif %}{{ _(person.get_reason_of_termination_display()) }}{% if person.died %}</strong>{% endif %}
                                    </td>
                                </tr>
                            {% else %}
                                {% set day_of_dismissal = person.day_of_dismissal %}
                                {% if day_of_dismissal %}
                                <tr>
                                    <td>{{ _("Дата звільнення") }}</td>
                                    <td>
                                        <time>{{ day_of_dismissal }}</time>
                                    </td>
                                </tr>
                                {% endif %}
                            {% endif %}


                            </tbody>
                        </table>
                    </div>
                </div>

                {% if scoring_flags and not person.died %}
                <div class="scoring">
                    <div class="scoring__header">
                        <div class="content">
                            <h6 class="scoring__title">{{ _("Скоринг ризику активів") }}</h6>
                            
                            <div class="progressbar">
                                <span class="index index--{{ scoring_score|round|int }}">{{ '%0.2f'| format(scoring_score|float) }}</span>
                            </div>

                            <button class="scoring__btn track-ga-event track-ga-event-click" data-ga-event-category="expand" data-ga-event-action="person_scoring">
                                {{ _("Розгорнути") }}
                                <svg>
                                    <use xlink:href="#arr-right-w2"></use>
                                </svg>
                            </button>
                        </div>
                    </div>
                    <div class="scoring__body content">
                        <h6 class="scoring__title">{{ _("ризики") }}</h6>
                        <ol class="scoring__list">
                            {% for flag in scoring_flags %}
                            <li>
                                <p>{{ flag.comment|safe }}</p>
                            </li>
                            {% endfor %}
                        </ol>
                        <p class="scoring__more">{{ _("Детальніше про методологію оцінювання ризику необгрунтованості доходів публічних діячів ") }}<a href="{{ url('wagtail_serve', 'scoring/') }}">{{ _("читайте тут") }}</a></p>
                    </div>
                </div>
                {% endif %}

                <div class="profile-page__blocks">
                    <div class="column-left">
                        {% if related_persons %}
                        <section id="relation_with_individual" class="profile-section">
                            <div class="profile-section__header">
                                <div class="title-wrap">
                                    <img src="{{ static('images/icon-individual.svg') }}" alt="icon" class="icon">
                                    <h6 class="title">{{ _('ЗВ’ЯЗКИ з фізичними особами') }}</h6>
                                </div>
                                {# <a href="" class="download">
                                    <svg>
                                        <use xlink:href="#arr-down"></use>
                                    </svg>
                                </a> #}
                            </div>
                            <div class="profile-section__body">
                                <table class="table sort-table" data-slice-count="1">
                                    <thead>
                                    <tr>
                                        <th class="no-wrap sort-head" data-colnum="col1">{{_('Iм’я')}}
                                            <svg class="sort-arr">
                                                <use xlink:href="#arr-right-w2"></use>
                                            </svg>
                                        </th>
                                        <th class="no-wrap sort-head" data-colnum="col2">{{_('Тип зв’язку')}}
                                            <svg class="sort-arr">
                                                <use xlink:href="#arr-right-w2"></use>
                                            </svg>
                                        </th>
                                        <th class="sort-head" data-colnum="col3">{{_('Перiод')}}
                                            <svg class="sort-arr">
                                                <use xlink:href="#arr-right-w2"></use>
                                            </svg>
                                        </th>
                                        <th>
                                        </th>
                                    </tr>
                                    </thead>
                                    <tbody>
                                        {{ render_related_grouped_persons(related_persons.family) }}
                                        {{ render_related_grouped_persons(related_persons.personal) }}
                                        {{ render_related_grouped_persons(related_persons.business) }}
                                    </tbody>
                                </table>
                            </div>
                            <div class="profile-section__footer">
                                <button class="more-btn track-ga-event track-ga-event-click" data-ga-event-category="expand" data-ga-event-action="person_related_persons">
                                    <span class="icon">
                                        <svg>
                                            <use xlink:href="#arr-right-w2"></use>
                                        </svg>
                                    </span>
                                    <span class="more">{{_("Показати більше")}}</span>
                                    <span class="less">{{_("Показати менше")}}</span>
                                </button>

                                <button class="visualization-btn track-ga-event track-ga-event-click" data-ga-event-category="expand" data-ga-event-action="person_connections_viz" data-target="#visualization-modal" data-url="{{ url('connections', 'person', person.pk) }}">
                                    <span class="icon">
                                        <svg>
                                            <use xlink:href="#visualization"></use>
                                        </svg>
                                    </span>
                                    {{_("Візуалізація зв’язків")}}
                                </button>
                            </div>
                        </section>
                        {% endif %}

                        {% if related_companies.rest %}
                        <section id="relation_with_entities" class="profile-section">
                            <div class="profile-section__header">
                                <div class="title-wrap">
                                    <img src="{{ static('images/icon-entities.svg') }}" alt="icon" class="icon">
                                    <h6 class="title">{{_("ЗВ’ЯЗКИ з юридичними особами")}}</h6>
                                </div>
                                {# <a href="" class="download">
                                    <svg>
                                        <use xlink:href="#arr-down"></use>
                                    </svg>
                                </a> #}
                            </div>
                            <div class="profile-section__body">
                                <table class="table sort-table" data-slice-count="1">
                                    <thead>
                                    <tr>
                                        <th class="no-wrap sort-head" data-colnum="col1">{{_("Iм’я")}}
                                            <svg class="sort-arr">
                                                <use xlink:href="#arr-right-w2"></use>
                                            </svg>
                                        </th>
                                        <th class="no-wrap sort-head" data-colnum="col2">{{_("Тип зв’язку")}}
                                            <svg class="sort-arr">
                                                <use xlink:href="#arr-right-w2"></use>
                                            </svg>
                                        </th>
                                        <th class="sort-head" data-colnum="col3">{{ _("Код") }}
                                            <svg class="sort-arr">
                                                <use xlink:href="#arr-right-w2"></use>
                                            </svg>
                                        </th>
                                        <th>
                                        </th>
                                    </tr>
                                    </thead>
                                    <tbody>
                                        {{ render_person_companies(related_companies.rest) }}
                                    </tbody>
                                </table>
                            </div>
                            <div class="profile-section__footer">
                                <button class="more-btn track-ga-event track-ga-event-click" data-ga-event-category="expand" data-ga-event-action="person_related_companies">
                                    <span class="icon">
                                        <svg>
                                            <use xlink:href="#arr-right-w2"></use>
                                        </svg>
                                    </span>
                                    <span class="more">{{_("Показати більше")}}</span>
                                    <span class="less">{{_("Показати менше")}}</span>
                                </button>

                                <button class="visualization-btn track-ga-event track-ga-event-click" data-ga-event-category="expand" data-ga-event-action="person_connections_viz"  data-target="#visualization-modal" data-url="{{ url('connections', 'person', person.pk) }}">
                                    <span class="icon">
                                        <svg>
                                            <use xlink:href="#visualization"></use>
                                        </svg>
                                    </span>
                                    {{_("Візуалізація зв’язків")}}
                                </button>
                            </div>
                        </section>
                        {% endif %}

                        {% if all_declarations %}
                        <section id="declarations" class="profile-section" data-chart-data="{{ charts_data.incomes|tojson|forceescape }}" data-chart-title="{% if LANGUAGE_CODE == "en" %}{{ _("USD") }}{% else %}{{ _("UAH") }}{% endif %}" data-chart-currency="{% if LANGUAGE_CODE == "en" %}${% else %}₴{% endif %}">
                            <div class="profile-section__header">
                                <div class="title-wrap">
                                    <img src="{{ static('images/icon-income-expenses.svg') }}" alt="icon" class="icon">
                                    <h6 class="title">{{_("доходи та витрати")}}</h6>
                                </div>
                                <div class="showing">
                                    <button class="show-table active">
                                        <svg>
                                            <use xlink:href="#show-table-icon"></use>
                                        </svg>
                                    </button>
                                    <button class="show-graph">
                                        <svg>
                                            <use xlink:href="#show-graph-icon"></use>
                                        </svg>
                                    </button>
                                </div>
                                {# <a href="" class="download">
                                    <svg>
                                        <use xlink:href="#arr-down"></use>
                                    </svg>
                                </a> #}
                            </div>
                            <div class="profile-section__body">
                                <table class="table sort-table" data-slice-count="1">
                                    <thead>
                                    <tr>
                                        <th class="no-wrap sort-head" data-colnum="col1">{{ _("Рiк") }}
                                            <svg class="sort-arr">
                                                <use xlink:href="#arr-right-w2"></use>
                                            </svg>
                                        </th>
                                        <th class="position sort-head" data-colnum="col2">{{_("Посада")}}
                                            <svg class="sort-arr">
                                                <use xlink:href="#arr-right-w2"></use>
                                            </svg>
                                        </th>
                                        <th class="sort-head" data-colnum="col3">{{_("Доходи декларанта")}}, 
                                            {% if LANGUAGE_CODE == "en" %}{{ _("USD") }}{% else %}{{ _("UAH") }}{% endif %}
                                            <svg class="sort-arr">
                                                <use xlink:href="#arr-right-w2"></use>
                                            </svg>
                                        </th>
                                        <th class="sort-head" data-colnum="col4">{{ _("Доходи родини") }},
                                            {% if LANGUAGE_CODE == "en" %}{{ _("USD") }}{% else %}{{ _("UAH") }}{% endif %}
                                            <svg class="sort-arr">
                                                <use xlink:href="#arr-right-w2"></use>
                                            </svg>
                                        </th>
                                        <th class="sort-head" data-colnum="col5">{{ _("Витрати та інші правочини декларанта") }}, {% if LANGUAGE_CODE == "en" %}{{ _("USD") }}{% else %}{{ _("UAH") }}{% endif %}
                                            <svg class="sort-arr">
                                                <use xlink:href="#arr-right-w2"></use>
                                            </svg>
                                        </th>
                                    </tr>
                                    </thead>
                                    <tbody>
                                    {% for declaration in all_declarations %}
                                        {% set income = declaration.get_income() %}
                                        <tr>
                                            <td class="no-wrap sort-col col1">
                                                <strong><a href="{{ income.url }}" target="_blank" data-toggle="tooltip" data-placement="bottom" title="{% if declaration.nacp_declaration %}{{ _('Електронна декларація') }}{% else %}{{ _('Паперова декларація') }}{% endif %}">{{ income.year }}</a></strong>
                                            </td>
                                            <td class="position sort-col col2" itemscope="" itemtype="http://schema.org/Organization" itemprop="worksFor">
                                                <p><span itemprop="name">{{ income.office }}</span>, {{ income.position }}{% if declaration.doc_type == "Кандидата на посаду" %} ({{ _("кандидат") }}){% endif %}</p>
                                            </td>
                                            <td class="no-wrap sort-col col3"><span>{{ income.income_of_declarant|convert_curr(income.year)|curformat() }}</span></td>
                                            <td class="no-wrap sort-col col4"><span>{{ income.income_of_family|convert_curr(income.year)|curformat() }}</span></td>
                                            <td class="no-wrap sort-col col5"><span>{{ income.expenses_of_declarant|convert_curr(income.year)|curformat() }}</span></td>
                                        </tr>
                                    {% endfor %}
                                    </tbody>
                                </table>

                                <div id="declarations-line-chart" class="chart"></div>

                                <div class="chart-popup" id="declarations-pie-popup">
                                    <div class="chart-popup__header">
                                        <span class="year"></span>
                                        <span class="title"></span>

                                        <button class="chart-popup__close">
                                            <svg>
                                                <use xlink:href="#close"></use>
                                            </svg>
                                        </button>
                                    </div>
                                    <div id="declarations-pie-chart"></div>
                                </div>
                            </div>
                            <div class="profile-section__footer">
                                <button class="more-btn track-ga-event track-ga-event-click" data-ga-event-category="expand" data-ga-event-action="person_declaration_income">
                                    <span class="icon">
                                        <svg>
                                            <use xlink:href="#arr-right-w2"></use>
                                        </svg>
                                    </span>
                                    <span class="more">{{_("Показати більше")}}</span>
                                    <span class="less">{{_("Показати менше")}}</span>
                                </button>
                            </div>
                        </section>
                        {% endif %}

                        {% if all_declarations %}
                        <section id="cash-assets" class="profile-section" data-chart-data="{{ charts_data.assets|tojson|forceescape }}" data-chart-title="{% if LANGUAGE_CODE == "en" %}{{ _("USD") }}{% else %}{{ _("UAH") }}{% endif %}">
                            <div class="profile-section__header">
                                <div class="title-wrap">
                                    <img src="{{ static('images/icon-cash-assets.svg') }}" alt="icon" class="icon">
                                    <h6 class="title">{{ _("Грошові активи:") }}</h6>
                                </div>
                                <div class="showing">
                                    <button class="show-table active">
                                        <svg>
                                            <use xlink:href="#show-table-icon"></use>
                                        </svg>
                                    </button>
                                    <button class="show-graph ">
                                        <svg>
                                            <use xlink:href="#show-graph-icon"></use>
                                        </svg>
                                    </button>
                                </div>
                                {# <a href="" class="download">
                                    <svg>
                                        <use xlink:href="#arr-down"></use>
                                    </svg>
                                </a> #}
                            </div>
                            <div class="profile-section__body">
                                <table class="table sort-table" data-slice-count="2">
                                    <thead>
                                    <tr>
                                        <th></th>
                                        <th></th>
                                        <th class="no-wrap" colspan="4">{{ _("Декларант") }}</th>
                                        <th class="no-wrap" colspan="4">{{ _("Родина") }}</th>
                                    </tr>
                                    <tr>
                                        <th class="no-wrap sort-head" data-colnum="col1">{{ _("Рік") }}
                                        <svg class="sort-arr">
                                                <use xlink:href="#arr-right-w2"></use>
                                            </svg>
                                        </th>
                                        <th class="no-wrap sort-head" data-colnum="col2">{{ _("Вид активу") }}
                                        <svg class="sort-arr">
                                                <use xlink:href="#arr-right-w2"></use>
                                            </svg>
                                        </th>
                                        <th class="no-wrap sort-head" data-colnum="col3">{{ _("UAH") }}
                                        <svg class="sort-arr">
                                                <use xlink:href="#arr-right-w2"></use>
                                            </svg>
                                        </th>
                                        <th class="no-wrap sort-head" data-colnum="col4">{{ _("USD") }}
                                        <svg class="sort-arr">
                                                <use xlink:href="#arr-right-w2"></use>
